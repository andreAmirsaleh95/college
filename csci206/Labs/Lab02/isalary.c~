#include <stdio.h>

int main(void) {
	
	int hourlyWage;	

	printf("Enter hourly wage: ");
	scanf("%d", &hourlyWage);
	printf("\nAnnual salary is: ");
	printf("%d", hourlyWage * 40 * 50);
	printf("\n");

	return 0;
}
